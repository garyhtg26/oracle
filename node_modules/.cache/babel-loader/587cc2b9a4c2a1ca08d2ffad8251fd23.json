alue) {\n            state.forms = value;\n        },\n        products(state, value) {\n            state.products = value;\n        },\n        items(state, value) {\n            state.items = value;\n        },\n        popup(state, value) {\n            state.popup = value\n        }\n    },\n    actions: {\n        updateForms({ commit, state }, data) {\n            var forms = state.forms;\n            forms[data.key] = data.value;\n            commit(\"forms\", forms);\n        },\n        async logout({ commit }) {\n            const res = axios.get(\"/auth/logout\");\n            commit(\"authentication\", false);\n            commit(\"token\", null);\n            commit(\"user\", { role: \"public\", id: 0 });\n            commit(\"code\", 200);\n            console.log(res);\n            // location.reload();\n            window.location = \"/\";\n        }\n    },\n    // actions,\n    // modules: {\n    //     auth,\n    //     cart,\n    //     products,\n    //     messages,\n    // },\n    plugins: [\n        createPersistedState({\n            storage: {\n                getItem: key => ls.get(key),\n                setItem: (key, value) => ls.set(key, value),\n                removeItem: key => ls.remove(key)\n            }\n        })\n    ]\n});"]}]}