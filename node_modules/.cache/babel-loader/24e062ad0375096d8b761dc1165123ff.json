,EAAElB,IAAI,CAACkB,KAAL,CAAWC,MAAX,CAAkB,CAAlB,EAAqB,CAArB,MAA4B,GAA5B,GAAkCnB,IAAI,CAACkB,KAAL,CAAWC,MAAX,CAAkB,CAAlB,CAAlC,GAAyDnB,IAAI,CAACkB;AAF5D,eADI;AAAA,gDAKVpB,KAAK,CAACK,IAAN,CAAW,gBAAX,EAA6BI,IAA7B,CALU;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMpB,GAhBU;AAiBLa,EAAAA,YAjBK,wBAiBQpB,IAjBR,EAiBcC,MAjBd,EAiBsBC,MAjBtB,EAiB8B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gDAC9BJ,KAAK,CAACK,IAAN,CAAW,oBAAX,EAAiCH,IAAjC,EAAuCI,IAAvC,CAA4C,UAAAC,GAAG,EAAI;AACtDJ,gBAAAA,MAAM,CAACK,MAAP,CAAc,gBAAd,EAAgCD,GAAG,CAACE,IAAJ,CAASC,MAAzC;AACAP,gBAAAA,MAAM,CAACK,MAAP,CAAc,MAAd,EAAsBD,GAAG,CAACE,IAAJ,CAASE,IAA/B;AACAX,gBAAAA,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IACI,YAAYX,MAAM,CAACY,KAAP,CAAaC,cAD7B;AAEAZ,gBAAAA,MAAM,CAACmB,IAAP,CAAY,GAAZ;AACH,eANM,CAD8B;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQxC,GAzBU;AA0BLC,EAAAA,MA1BK,kBA0BErB,MA1BF,EA0BUC,MA1BV,EA0BkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gDAClBJ,KAAK,CAACyB,GAAN,CAAU,aAAV,EAAyBnB,IAAzB,CAA8B,YAAM;AACvCH,gBAAAA,MAAM,CAACK,MAAP,CAAc,gBAAd,EAAgC,EAAhC;AACAL,gBAAAA,MAAM,CAACK,MAAP,CAAc,MAAd,EAAsB,EAAtB;AACAR,gBAAAA,KAAK,CAACY,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiDY,SAAjD;AACAtB,gBAAAA,MAAM,CAACuB,OAAP,CAAe,GAAf;AACH,eALM,CADkB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAO5B;AAjCU,CAAf","sourcesContent":["import axios from \"axios\"\n// import stores from \"../stores/index\"\n\nexport default {\n    async login(form, stores, router) {\n        return axios.post('/auth/login', form).then(res => {\n            stores.commit('authentication', res.data.bearer)\n            stores.commit('user', res.data.user)\n            axios.defaults.headers.common['Authorization'] =\n                'Bearer ' + stores.state.authentication\n            router.go(-1);\n        })\n    },\n    async register(form) {\n        const data = {\n            name: form.name || form.phone,\n            phone: form.phone.substr(0, 1) === \"0\" ? form.phone.substr(1) : form.phone\n        }\n        return axios.post('/auth/register', data)\n    },\n    async verification(form, stores, router) {\n        return axios.post('/auth/verification', form).then(res => {\n            stores.commit('authentication', res.data.bearer)\n            stores.commit('user', res.data.user)\n            axios.defaults.headers.common['Authorization'] =\n                'Bearer ' + stores.state.authentication\n            router.push('/');\n        })\n    },\n    async logout(stores, router) {\n        return axios.get('auth/logout').then(() => {\n            stores.commit('authentication', {})\n            stores.commit('user', {})\n            axios.defaults.headers.common['Authorization'] = undefined\n            router.replace('/');\n        })\n    }\n}"]}]}