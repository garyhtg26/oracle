R,IAAI,CAACS,WAJF;AAKVC,gBAAAA,QAAQ,EAAEV,IAAI,CAACW,QALL;AAMVC,gBAAAA,MAAM,EAAE,SANE;AAOVC,gBAAAA,OAAO,EAAEb,IAAI,CAACa,OAPJ;AAQVC,gBAAAA,SAAS,EAAEd,IAAI,CAACc;AARN,eADA;AAAA,gDAWPpC,KAAK,CAACqC,GAAN,CAAU,mBAAV,EAA+Bd,KAA/B,CAXO;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYjB,GA5EU;AA6ELe,EAAAA,MA7EK,kBA6EEf,KA7EF,EA6ESrB,MA7ET,EA6EiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gDACjBF,KAAK,CAACqC,GAAN,CAAU,QAAV,EAAoBd,KAApB,EAA2B;AAC9BgB,gBAAAA,OAAO,EAAE;AACLC,kBAAAA,aAAa,mBAAYtC,MAAM,CAACG,KAAP,CAAaoC,cAAzB;AADR;AADqB,eAA3B,CADiB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAM3B,GAnFU;AAoFLC,EAAAA,UApFK,sBAoFMnB,KApFN,EAoFarB,MApFb,EAoFqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gDACrBF,KAAK,CAACqC,GAAN,CAAU,aAAV,EAAyBd,KAAzB,EAAgC;AACnCgB,gBAAAA,OAAO,EAAE;AACLC,kBAAAA,aAAa,mBAAYtC,MAAM,CAACG,KAAP,CAAaoC,cAAzB;AADR;AAD0B,eAAhC,CADqB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAM/B;AA1FU,CAAf","sourcesContent":["import axios from 'axios'\nexport default {\n    isDisabled($store, channel, code) {\n        return (\n            $store.state.payments.filter((x) => x[channel] === code && x.is_enabled)\n            .length == 0\n        )\n    },\n    optPayments($store, selected) {\n        return [{\n                img: 'alfamart',\n                code: 'ALFAMART',\n                disabled: this.isDisabled($store, 'channel_code', 'ALFAMART'),\n            },\n            {\n                img: 'ovo',\n                code: 'OVO',\n                disabled: this.isDisabled($store, 'channel_code', 'OVO'),\n            },\n            {\n                img: 'dana',\n                code: 'DANA',\n                disabled: this.isDisabled($store, 'channel_code', 'DANA'),\n            },\n            {\n                img: 'shopeepay',\n                code: 'SHOPEEPAY',\n                disabled: this.isDisabled($store, 'channel_code', 'SHOPEEPAY'),\n            },\n            {\n                img: 'LINKAJA',\n                code: 'LINKAJA',\n                disabled: this.isDisabled($store, 'channel_code', 'LINKAJA'),\n            },\n            {\n                img: 'QRIS',\n                code: 'QRIS',\n                disabled: this.isDisabled($store, 'channel_code', 'QRIS'),\n            },\n            {\n                img: 'atm',\n                code: 'VIRTUAL_ACCOUNT',\n                disabled: this.isDisabled(\n                    $store,\n                    'channel_category',\n                    'VIRTUAL_ACCOUNT',\n                ) || !selected.enable_va,\n            },\n        ]\n    },\n    async getVav() {\n        return axios.get('voucher/va')\n    },\n    async payments() {\n        return axios.get('xendit/payment_channels')\n    },\n    async index() {\n        return axios.get('transaction')\n    },\n    async callback() {\n        return axios.get('transaction/callback')\n    },\n    async report() {\n        return axios.get('transaction/report')\n    },\n    async store(data) {\n        const forms = {\n            buyer_sku_code: data.pulsa_code,\n            customer_no: data.no_hp,\n            customer_id: data.hp,\n            price: data.pulsa_price,\n            category: data.pulsa_op,\n            status: 'Pending',\n            payment: data.payment,\n            server_id: data.server_id,\n        }\n        return axios.put('transaction/topup', forms)\n    },\n    async redeem(forms, $store) {\n        return axios.put('redeem', forms, {\n            headers: {\n                Authorization: `Bearer ${$store.state.authentication}`,\n            },\n        })\n    },\n    async redeemCode(forms, $store) {\n        return axios.put('redeem-code', forms, {\n            headers: {\n                Authorization: `Bearer ${$store.state.authentication}`,\n            },\n        })\n    },\n}"]}]}