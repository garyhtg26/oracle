 }\n        return axios.post('transaction/topup', forms)\n    },\n    async redeem(forms, $store) {\n        return axios.post('redeem', forms, {\n            headers: {\n                'Authorization': `Bearer ${$store.state.authentication}`\n            }\n        })\n    }\n}"]}]}