ns } from \"vuex\";\nimport auth from \"@/controller/auth.js\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      isLoading: false,\n    };\n  },\n  methods: {\n    ...mapActions([\"addMessage\", \"clearMessage\", \"loginWithEmail\"]),\n    onSubmit() {\n      this.isLoading = true;\n      let data = {\n        phone: this.email,\n        // password: this.password,\n      };\n      auth.login(data, this.$store, this.$router);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.login-container {\n  width: 100%;\n  justify-content: center;\n}\n\n.btn-home {\n  margin-top: 25px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  color: grey;\n}\n</style>\n\n"]}]}